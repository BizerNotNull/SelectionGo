{
  "manifest_version": 3,
  "name": "SelectionGo",
  "description": "Right-click selected text to search with customizable providers and menus.",
  "version": "1.0.1",
  "permissions": [
    "contextMenus",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "SelectionGo",
    "default_popup": "popup.html"
  }
}
